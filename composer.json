{
    "name": "zendframework/skeleton-application",
    "description": "Skeleton Application for Zend Framework zend-mvc applications",
    "type": "project",
    "license": "BSD-3-Clause",
    "keywords": [
        "framework",
        "mvc",
        "zf"
    ],
    "homepage": "http://framework.zend.com/",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "zfcampus/zf-console": "^1.4",
        "zendframework/zend-mvc": "^3.1",
        "zendframework/zend-log": "^2.9",
        "neeckeloo/monolog-module": "^0.4",
        "chobie/jira-api-restclient": "^2.0@dev",
        "ajt/guzzle-toggl": "dev-master",
        "zendframework/zend-hydrator": "^2.3",
        "zendframework/zend-db": "^2.9"
    },
    "require_dev": {
        "phpunit/phpunit": "^7.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "TogglJira\\": "module/TogglJira/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TogglJiraTest\\": "module/TogglJira/test/"
        }
    }
}
